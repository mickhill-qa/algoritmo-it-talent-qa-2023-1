algoritmo "Nome do Programa"
var
    x: real
    y: inteiro
    a: caractere
    b: logico

inicio
    x <- 9.2
    escreval("x: real = " + x)

    y <- 1
    y <- 2
    y <- 50
    y <- 10000
    escreval("y: inteiro = " + y)

    a <- "Ola Mundo!"
    escreval("a: caractere = " + a)

    b <- verdadeiro
    escreval("b: logico = " + b)

fimalgoritmo
