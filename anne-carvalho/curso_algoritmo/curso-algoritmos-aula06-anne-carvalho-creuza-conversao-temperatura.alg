Algoritmo "Creuza Conversao de Temperatura"
Var
   F,C : Real

Inicio
      Escreva("Qual e a temperatura aqui ")
      Leia(F)
      C <- (F - 32)/1.8
      Escreva("No Brasil, estaria", C:4:1)

Fimalgoritmo
