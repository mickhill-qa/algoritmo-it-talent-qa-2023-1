Algoritmo "Creuza-converter-moeda"

// Autor(a)    : Yuri Carvalho Teixeira
// Data atual  : 27/05/2023
Var
   moeda_real, dolar: real



Inicio
      escreval("Informe o valor em real:")
      leia(moeda_real)
      dolar<- moeda_real / 4.99
      escreva("Valor em dolares: ",dolar)



Fimalgoritmo