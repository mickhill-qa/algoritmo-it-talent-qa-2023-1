algoritmo "Creuza01"

var

    ano_atual, ano_nascimento, idade: inteiro

inicio

    Escreval ("Informe o ano que nós estamos?")
    Leia(ano_atual)
    Escreval("Em que ano você nasceu?")
    Leia(ano_nascimento)
    idade<- ano_atual - ano_nascimento
    Escreva("A idade de Creuza é ", idade, " anos.")
fimalgoritmo